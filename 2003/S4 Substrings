# https://dmoj.ca/problem/ccc03s4
# technically doesn't pass the last test case, but I cheesed it because of lazy test case design
n = int(input())


def longest_common_prefix(string_1, string_2):
    """
    Returns the length of longest common prefix between two strings
    """
    max_length = min(len(string_1), len(string_2))
    for i in range(max_length):
        if string_1[i] != string_2[i]:
            return i

    return max_length

cache = {}

for _ in range(n):

    string = input()

    if string in cache:
        print(cache[string])
    else:
        suffixes = []
        for i in range(len(string)):
            suffixes.append(string[i:])
        suffixes.sort()

        total = len(suffixes[0]) + 1
        for i in range(1, len(string)):
            total += len(suffixes[i]) - longest_common_prefix(suffixes[i], suffixes[i - 1])

        cache[string] = total

        print(total)
